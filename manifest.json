{
	// REQUIRED
	
		"manifest_version": 2,
		"name": "MB3 Upcoming",
		"version": "0.1",
  
	// RECOMMENDED
	
		"description": "This extension displays the latest upcoming episodes from your Media Browser Server.",
		"author": "Angelblue05",
		"icons":{
			"128": "Icons/icon_128.png",
			"48": "Icons/icon_48.png",
			"16": "Icons/icon_16.png"
		},
  
	// IMPORTANT
	
		"browser_action": {
			"default_icon": "icon.png",
			"default_popup": "popup.html"
		},
  
	// SECURITY
	
		"content_security_policy": "script-src 'self'; object-src 'self'"
}
