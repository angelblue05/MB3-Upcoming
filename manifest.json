{
	"manifest_version": 2,
	"name": "MB3 Upcoming",
	"version": "0.1",

	"description": "This extension displays the latest upcoming episodes populated in Media Browser 3.",
	"author": "angelblue05",

	"icons": {
		"128": "Icons/icon_128.png",
		"48": "Icons/icon_48.png",
		"16": "Icons/icon_16.png"
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permission":  ["storage"],
	"content_security_policy": "script-src 'self'; object-src 'self'"
}
